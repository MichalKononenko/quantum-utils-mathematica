          RowBox[{"0", ",", "0"}], "}"}], ",", 
         RowBox[{"{", 
          RowBox[{"0", ",", "1"}], "}"}]}], "}"}]}], ",", 
      RowBox[{"\"\<H\>\"", "\[Rule]", 
       RowBox[{"{", 
        RowBox[{
