       RowBox[{
        SubscriptBox["a", "2"], "(", 
        SubscriptBox["t", "0"], ")"}], ",", "...", ",", 
       RowBox[{
        SubscriptBox["a", "n"], "(", 
        SubscriptBox["t", "0"], ")"}]}], "}"}], ",", 
     RowBox[{"{", 
