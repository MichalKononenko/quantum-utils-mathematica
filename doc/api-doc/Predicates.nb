Cell[CellGroupData[{

Cell["Example 3: Functions", "Subsubsection",
 CellChangeTimes->{{3.635554228897019*^9, 3.635554254441828*^9}}],

Cell["\<\
Symbolic functions (those where the function symbol does not have any \
definitions) are recognized as coefficients, regardless of depth of \
concatenated functions\
\>", "Text",
 CellChangeTimes->{{3.6355542647052937`*^9, 3.6355542803712*^9}, {
  3.635554361332555*^9, 3.6355543690462837`*^9}, {3.6355546795526237`*^9, 
  3.635554702519195*^9}}],

Cell[CellGroupData[{

Cell[BoxData[
 RowBox[{"CoefficientQ", "/@", 
  RowBox[{"{", 
   RowBox[{
    RowBox[{"a", "[", 
     RowBox[{"1", ",", "2"}], "]"}], ",", 
    RowBox[{"f", "[", "x", "]"}], ",", " ", 
    RowBox[{"g", "[", 
     RowBox[{"f", "[", "x", "]"}], "]"}], ",", 
    RowBox[{"h", "@", 
     RowBox[{"g", "@", 
      RowBox[{"f", "@", "x"}]}]}]}], "}"}]}]], "Input",
 CellChangeTimes->{{3.635176242324267*^9, 3.635176248626647*^9}, {
  3.635554336948444*^9, 3.63555437710406*^9}}],

Cell[BoxData[
 RowBox[{"{", 
  RowBox[{"True", ",", "True", ",", "True", ",", "True"}], "}"}]], "Output",
 CellChangeTimes->{
  3.635176248892572*^9, {3.63555433149946*^9, 3.6355543784604588`*^9}}]
}, Open  ]],

Cell["\<\
If the symbol has any associated DownValues it will evaluate to false (if the \
function doesn\[CloseCurlyQuote]t explicitly evaluate to a valid Coefficient \
quantitiy).\
\>", "Text",
 CellChangeTimes->{{3.635554963350359*^9, 3.63555497833694*^9}, {
  3.63555503950493*^9, 3.635555055386857*^9}}],

Cell[CellGroupData[{

Cell[BoxData[
 RowBox[{"Block", "[", 
  RowBox[{
   RowBox[{"{", "f", "}"}], ",", "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{
     RowBox[{"f", "[", "x_", "]"}], ":=", 
     RowBox[{"3", "x"}]}], ";", "\[IndentingNewLine]", 
    RowBox[{"CoefficientQ", "/@", 
     RowBox[{"{", 
      RowBox[{"f", ",", 
